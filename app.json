{
  "expo": {
    "name": "SecuVision",
    "slug": "secuvision",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "681a4513-6e19-4dcd-8c8c-5f1e22f80f3e"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.amhar_2025.secuvision"
    }
  }
}
